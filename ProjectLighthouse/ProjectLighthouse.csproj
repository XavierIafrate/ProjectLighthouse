  <ItemGroup>
    <None Remove="Resources\done.png" />
    <None Remove="Resources\GIF\0.gif" />
    <None Remove="Resources\GIF\excited.gif" />
    <None Remove="Resources\GIF\moderate.gif" />
    <None Remove="Resources\GIF\no.gif" />
    <None Remove="Resources\LighthouseLogo.png" />
    <None Remove="Resources\Lighthouse_dark.png" />
    <None Remove="Resources\Logo.png" />
    <None Remove="Resources\MO-1star.png" />
    <None Remove="Resources\MO-2star.png" />
    <None Remove="Resources\MO-3star.png" />
    <None Remove="Resources\MO-Notify.png" />
    <None Remove="Resources\MO-SpecialPartWarning.png" />
    <None Remove="Resources\MO-Stop.png" />
    <None Remove="Resources\MO-Warn.png" />
