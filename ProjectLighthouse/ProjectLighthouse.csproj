    <PackageReference Include="WpfAnimatedGif" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\0.gif">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\GIF\excited.gif" />
    <Resource Include="Resources\GIF\moderate.gif" />
    <Resource Include="Resources\GIF\no.gif" />
    <Resource Include="Resources\LighthouseLogo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Lighthouse_dark.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Logo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\MO-1star.png" />
    <Resource Include="Resources\MO-2star.png" />
    <Resource Include="Resources\MO-3star.png" />
    <Resource Include="Resources\done.png" />
    <Resource Include="Resources\MO-Notify.png" />
    <Resource Include="Resources\MO-SpecialPartWarning.png" />
    <Resource Include="Resources\MO-Stop.png" />
    <Resource Include="Resources\MO-Warn.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="Resources\Font\Proxima Nova Alt Bold.otf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Resources\Font\Proxima Nova Alt Light.otf">
