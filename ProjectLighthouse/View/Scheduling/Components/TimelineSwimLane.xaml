<UserControl
    x:Class="ProjectLighthouse.View.Scheduling.Components.TimelineSwimLane"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ProjectLighthouse.View.Scheduling.Components"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid
        x:Name="MainGrid"
        AllowDrop="True"
        Background="White"
        DragEnter="MainGrid_DragEnter"
        DragLeave="MainGrid_DragLeave"
        DragOver="MainGrid_DragOver"
        Drop="MainGrid_Drop"
        GiveFeedback="MainGrid_GiveFeedback" />
</UserControl>
