<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="Default" TargetType="ToggleButton">
        <Setter Property="Background" Value="{StaticResource DisabledElement}" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="Margin" Value="10,5" />
        <Setter Property="Foreground" Value="{StaticResource OnBackground}" />
        <!--<Setter Property="FocusVisualStyle" Value="{x:Null}"/>-->
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Border
                        Height="24"
                        Padding="20,0"
                        Background="{TemplateBinding Background}"
                        BorderThickness="0"
                        CornerRadius="12">
                        <ContentPresenter
                            Margin="0,0,0,2"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource OnBlue}" />
                            <Setter Property="Background" Value="{StaticResource Blue}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--  Don't touch  -->
    <Style BasedOn="{StaticResource Default}" TargetType="ToggleButton" />



    <Style x:Key="MenuItem" TargetType="ToggleButton">
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="Padding" Value="30,0" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Margin" Value="10,5" />
        <Setter Property="Foreground" Value="#888888" />
        <Setter Property="FontSize" Value="20" />
        <Setter Property="Background" Value="{StaticResource Surface}" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />


        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{StaticResource PurpleLight}" />
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Foreground" Value="{StaticResource Purple}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="LightGray" />
            </Trigger>
        </Style.Triggers>
    </Style>

